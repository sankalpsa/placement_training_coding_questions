// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i,n;
    scanf("%d",&n);
    int array[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    i=0;
    while(i<n-1)
    {
        if(array[i]==0)
        {
            break;
        
        }
        i+=array[i];

    }
    if(i=n-1)
        printf("True");
    else
        printf("False");
    return 0;
}


